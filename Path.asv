function count = Path(vector)
    count = 0;
    procent = 0.0;
    pathLength = 0;
    while procent < 0.8
        OnePath(vector);
        InitializeTheta();
        vector = CalculateDirectionVector(vector, pathLength);
        procent = CleanArea()
        count = count + 1;
        %if mod(count,10) == 0
            PlotSim()
        %end
    end
end